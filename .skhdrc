cmd + alt - g : toggle-grayscale
cmd + shift - 9 : /usr/bin/osascript -e 'do shell script "killall coreaudiod 2>&1" with administrator privileges'
cmd + shift - 0 : launchctl kickstart -k "gui/${UID}/homebrew.mxcl.yabai"

# focus specific window
cmd + shift - a : yabai -m query --windows | jq '.[] | select(.app=="Alacritty").space' | xargs yabai -m space --focus || yabai -m space --focus recent
cmd + shift - d : yabai -m query --windows | jq '.[] | select(.app=="Slack").space' | xargs yabai -m space --focus || yabai -m space --focus recent
cmd + shift - s : yabai -m query --windows | jq '.[] | select(.app=="Firefox").space' | xargs yabai -m space --focus || yabai -m space --focus recent
cmd + shift - w : yabai -m query --windows | jq '.[] | select(.app=="Messages").space' | xargs yabai -m space --focus || yabai -m space --focus recent
cmd + shift - l : yabai -m query --windows | jq '.[] | select(.app=="Music").space' | xargs yabai -m space --focus || yabai -m space --focus recent
cmd + shift - m : yabai -m query --windows | jq '.[] | select(.app=="Mimestream").space' | xargs yabai -m space --focus || yabai -m space --focus recent
cmd + shift - z : yabai -m query --windows | jq '.[] | select(.app=="zoom.us").space' | xargs yabai -m space --focus || yabai -m space --focus recent
cmd + shift - x : yabai -m query --windows | jq '.[] | select(.app=="Xcode").space' | xargs yabai -m space --focus || yabai -m space --focus recent

# Space navigation
cmd + shift - left : yabai -m space --focus prev || yabai -m space --focus last
cmd + shift - right : yabai -m space --focus next || yabai -m space --focus first
cmd + shift - down : yabai -m space --destroy
cmd + shift - up : yabai -m space --create

.blacklist [
    "simulator"
]
