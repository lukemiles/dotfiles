cmd + alt - g : toggle-grayscale
cmd + shift - 9 : /usr/bin/osascript -e 'do shell script "killall coreaudiod 2>&1" with administrator privileges'
cmd + shift - 0 : brew services restart yabai

# focus specific window
cmd + shift - a : yabai -m query --windows | jq '.[] | select(.app=="Alacritty").space' | xargs yabai -m space --focus || yabai -m space --focus recent
cmd + shift - d : yabai -m query --windows | jq '.[] | select(.app=="Slack").space' | xargs yabai -m space --focus || yabai -m space --focus recent
cmd + shift - s : yabai -m query --windows | jq '.[] | select(.app=="Firefox").space' | xargs yabai -m space --focus || yabai -m space --focus recent
cmd + shift - w : yabai -m query --windows | jq '.[] | select(.app=="Messages").space' | xargs yabai -m space --focus || yabai -m space --focus recent

.blacklist [
    "simulator"
]
